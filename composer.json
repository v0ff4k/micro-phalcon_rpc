{
    "name": "vladimir/micro_phalcon-start",
    "description": "My 1st start with phalcon and docker. Using micro phalcon with json-rpc connecction",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "public/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=7.1",
        "ext-phalcon": "~3.0",
        "phalcon/cphalcon": "3.4.2",
        "emog/phalcon-jwt-auth": "^1.0"
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR2"
    },
    "license": "proprietary",
    "authors": [{"name": "Vladimir Che", "email": "v0ff@bk.ru"}],
    "minimum-stability": "beta",
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phalcon/incubator": "^3.4",
        "phalcon/dd": "^1.1"
    }
}
